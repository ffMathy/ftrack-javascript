{
  "name": "ftrack-javascript-api",
  "version": "0.2.0",
  "description": "JavaScript API for ftrack.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack --progress --colors --watch --env=dev",
    "dist": "webpack --env=dist",
    "lint": "eslint ./source",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "clean": "rimraf ./lib",
    "babel": "babel ./source --out-dir ./lib",
    "build": "cross-env NODE_ENV=production npm run babel"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.1.18",
    "babel-loader": "^6.1.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "latest",
    "babel-preset-es2015": "^6.3.13",
    "chai": "latest",
    "chai-as-promised": "^5.3.0",
    "cross-env": "^2.0.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "exports-loader": "latest",
    "imports-loader": "latest",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "latest",
    "phantomjs-prebuilt": "latest",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0",
    "yargs": "^3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/ftrack/ftrack-javascript-api.git"
  },
  "keywords": [
    "ftrack",
    "api",
    "library",
    "umd"
  ],
  "author": "ftrack",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://bitbucket.org/ftrack/ftrack-javascript-api/issues"
  },
  "homepage": "http://ftrack.com",
  "dependencies": {
    "es6-promise": "^3.1.2",
    "lodash": "^4.12.0",
    "loglevel": "^1.4.0",
    "moment": "^2.13.0",
    "socket.io-client": "^0.9.7",
    "uuid": "^2.0.2",
    "whatwg-fetch": "^1.0.0"
  }
}
